{
  "name": "@demoproject/auth-lambda",
  "version": "1.0.0",
  "description": "Authentication Lambda functions for DemoProject",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:unit": "jest --testPathPattern=__tests__/.*\\.test\\.ts",
    "test:integration": "jest --testPathPattern=__tests__/.*\\.integration\\.test\\.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "package": "npm run build && zip -r auth-lambda.zip dist/ node_modules/",
    "clean": "if exist dist rmdir /s /q dist && if exist coverage rmdir /s /q coverage && if exist node_modules rmdir /s /q node_modules && if exist package-lock.json del package-lock.json",
    "prebuild": "npm run clean",
    "pretest": "npm run build"
  },
  "dependencies": {
    "aws-sdk": "^2.1531.0",
    "aws-jwt-verify": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.133",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "keywords": [
    "aws",
    "lambda",
    "cognito",
    "authentication",
    "jwt",
    "demoproject"
  ],
  "author": "DemoProject Team",
  "license": "MIT"
}
