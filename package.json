{
  "name": "demo-project",
  "version": "0.1.0",
  "private": true,
  "description": "A modern Next.js application with AWS CDK infrastructure as code",
  "keywords": [
    "nextjs",
    "typescript",
    "tailwind",
    "aws-cdk",
    "infrastructure"
  ],
  "author": "Development Team",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "dev:regular": "next dev",
    "build": "next build",
    "build:standalone": "next build && next export",
    "monitor": "node connection-safeguards.js",
    "monitor:cron": "./monitor-aws-services.sh --cron",
    "health-check": "./monitor-aws-services.sh",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .next out dist",
    "clean:win": "if exist .next rmdir /s .next && if exist out rmdir /s out && if exist dist rmdir /s dist",
    "docker:build": "docker build -t demoproject-app .",
    "docker:run": "docker run -p 3000:3000 demoproject-app",
    "docker:push": "docker tag demoproject-app:latest $ECR_REPOSITORY_URI:latest && docker push $ECR_REPOSITORY_URI:latest",
    "infra:build": "cd cdk && npm run build",
    "infra:deploy": "cd cdk && npm run deploy:all",
    "infra:destroy": "cd cdk && npm run destroy:all",
    "infra:diff": "cd cdk && npm run diff",
    "infra:status": "cd cdk && npm run status",
    "infra:install": "cd cdk && npm install"
  },
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.890.0",
    "@aws-sdk/client-cloudwatch": "^3.890.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.890.0",
    "@aws-sdk/client-cognito-identity": "^3.890.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.890.0",
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-lambda": "^3.890.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-ses": "^3.890.0",
    "@aws-sdk/client-sns": "^3.890.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "@heroicons/react": "^2.2.0",
    "@testing-library/dom": "^10.4.1",
    "@tiptap/pm": "^3.2.0",
    "@tiptap/react": "^3.2.0",
    "@tiptap/starter-kit": "^3.2.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react-datepicker": "^6.2.0",
    "aws-sdk": "^2.1692.0",
    "dotenv": "^17.2.2",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "next": "15.4.6",
    "openai": "^5.20.3",
    "react": "19.1.0",
    "react-datepicker": "^8.5.0",
    "react-dom": "19.1.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/demo-project.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/demo-project/issues"
  },
  "homepage": "https://github.com/your-username/demo-project#readme",
  "main": "check-dns.js",
  "directories": {
    "doc": "docs"
  }
}
